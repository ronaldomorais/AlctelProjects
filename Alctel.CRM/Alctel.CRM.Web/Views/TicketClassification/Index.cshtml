@{
	var itemcounter = ViewBag.StartIndex;

	if (itemcounter == null)
	{
		itemcounter = 1;
	}

	string? baseUrl = Context.Session.GetString("BaseUrl");

	baseUrl = baseUrl == null ? string.Empty : baseUrl;
}

@model Alctel.CRM.Web.Models.TicketClassificationModel;

<div class="card">
	<div class="card-header">

		<div class="row">
			<div class="col-sm-6">

				<svg xmlns="http://www.w3.org/2000/svg" class="bi bi-person-lines-fill" viewBox="0 0 16 16">
					<path d="M6 8a3 3 0 1 0 0-6 3 3 0 0 0 0 6m-5 6s-1 0-1-1 1-4 6-4 6 3 6 4-1 1-1 1zM11 3.5a.5.5 0 0 1 .5-.5h4a.5.5 0 0 1 0 1h-4a.5.5 0 0 1-.5-.5m.5 2.5a.5.5 0 0 0 0 1h4a.5.5 0 0 0 0-1zm2 3a.5.5 0 0 0 0 1h2a.5.5 0 0 0 0-1zm0 3a.5.5 0 0 0 0 1h2a.5.5 0 0 0 0-1z" />
				</svg>
				<label class="label-title">Configuração da Classificação</label>


			</div>
			<div class="col-sm-3">
				<a href="@baseUrl/TicketClassification/Create">Novo</a>

			</div>
		</div>

	</div>

	<div class="card-body">

		<div class="row" style="margin-bottom:10px;">

			Index

 		</div>

	</div>

</div>

@section Scripts {
	<script src="~/js/ticketClassificationConfig.js"></script>
}
